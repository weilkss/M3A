@import "./common.wxss";

page {
  display: flex;
  flex-direction: column;
  height: 100vh;
}

.video-box {
  height: 200px;
  background-color: #000;
  font-size: 0;
  text-align: center;
}

.video {
  width: 100%;
  height: 200px;
}

.video-user {
  margin-bottom: 30rpx;
}

.content-box {
  height: calc(100vh - 280px - 30px);
}
