         url('//at.alicdn.com/t/font_1523011_t98rorrw1sq.woff2') format('woff2'),
         url('//at.alicdn.com/t/font_1523011_t98rorrw1sq.woff') format('woff'),
         url('//at.alicdn.com/t/font_1523011_t98rorrw1sq.ttf') format('truetype'),
         url('//at.alicdn.com/t/font_1523011_t98rorrw1sq.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  display: inline-block;
  vertical-align: middle;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

