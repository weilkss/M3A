@import "./components/toast/index.wxss";

page {
  background: #fff;
  -webkit-overflow-scrolling: touch;
}

button {
  background-color: transparent;
  border: none;
  outline: none;
  margin: 0;
  padding: 0;
  text-align: center;
  line-height: 1;
  font-size: 0;
}

button:focus {
  outline: 0;
}

button::after {
  border: none;
}

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.muEllipsis {
  overflow: hidden;
  word-wrap: break-word;
  text-overflow: ellipsis;
  display: -webkit-box;
  /*! autoprefixer: off */
  -webkit-box-orient: vertical;
  /* autoprefixer: on */
  -webkit-line-clamp: 2;
}

.transition {
  opacity: 0.2;
  animation: transition 1s forwards;
}

@keyframes transition {
  0% {
    opacity: 0.2;
  }

  100% {
    opacity: 1;
  }
}

@font-face {
  font-family: 'iconfont'; /* project id 1523011 */
  src: url('//at.alicdn.com/t/font_1523011_t98rorrw1sq.eot');
